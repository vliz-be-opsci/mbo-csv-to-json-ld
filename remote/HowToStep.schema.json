        {
            "name": "url",
            "titles": {
                "en": [
                    "URL"
                ]
            },
            "dc:description": {
                "en": [
                    "Must include the protocol (scheme) and should follow RFC 3986 standards\n\nFor example: `https://example.com`, `http://example.com`\n`ftp://ftp.example.org/data`, `doi:10.1000/example123`, `urn:isbn:1234567890`, `mailto:contact@example.com`"
                ]
            },
            "propertyUrl": "https://schema.org/url",
            "datatype": {
                "@id": "https://schema.org/URL",
                "base": "string"
            }
        },
        {
